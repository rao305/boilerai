DATABASE_URL=postgresql://app:app@localhost:5432/boilerai

# RAG Configuration
ENABLE_RAG=1
STRICT_INGEST=1

# LLM Provider Configuration
LLM_PROVIDER=gemini
GEMINI_API_KEY=__PUT_YOURS__
GEMINI_MODEL=gemini-2.0-flash-exp
OPENAI_API_KEY=__PUT_YOURS__
OPENAI_MODEL=gpt-4o-mini

# Embedding Provider Configuration  
EMBED_PROVIDER=gemini
EMBED_MODEL=text-embedding-004
# Alternative: text-embedding-3-large (OpenAI) or all-MiniLM-L6-v2 (local)

# Vector Database Configuration
VECTOR_BACKEND=qdrant
QDRANT_URL=http://localhost:6333
# Alternative: pgvector (use existing PostgreSQL with vector extension)

# Legacy Configuration
LLM_JSON_STRICT=1 